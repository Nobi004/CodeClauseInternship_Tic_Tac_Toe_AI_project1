{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      },
      {
        "src": "game/static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "game/static/$1" },
      { "src": "/(.*)", "dest": "main.py" }
    ]
  }
  